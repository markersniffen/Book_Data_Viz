class Datapoint { // like my old 'book' class
  
  ArrayList<String> strVals = new ArrayList<String>();
  ArrayList<Integer> intVals = new ArrayList<Integer>();
  
  Datapoint() {
    ArrayList<String> strVals = new ArrayList<String>();
    ArrayList<Integer> intVals = new ArrayList<Integer>();
  }

  void strData(String input_) {
    strVals.add(input_);
  }
  
  void intData(int input_) {
    intVals.add(input_);
  }
  

  
  
  
  
  
}
